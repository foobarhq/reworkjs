{
  "name": "react-framework",
  "version": "1.0.0",
  "description": "A framework based on react-boilerplate 3.1.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/mxstbr/react-boilerplate.git"
  },
  "engines": {
    "npm": ">=3"
  },
  "author": "Guylian Cox",
  "license": "MIT",
  "scripts": {
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./framework/internals/scripts/analyze.js",
    "analyze:clean": "rimraf stats.json",
    "npmcheckversion": "node ./framework/internals/scripts/npmcheckversion.js",
    "preinstall": "npm run npmcheckversion",
    "lint": "npm run lint:eslint && npm run lint:css",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --ignore-pattern public/",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test karma start internals/testing/karma.conf.js --single-run",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "test:firefox": "npm run test -- --browsers Firefox",
    "test:safari": "npm run test -- --browsers Safari",
    "test:ie": "npm run test -- --browsers IE",
    "build:internals": "babel framework --out-dir=.build --copy-files",
    "build:internals:watch": "npm run build:internals -- --watch",
    "coveralls": "cat ./coverage/lcov/lcov.info | coveralls",
    "build:watch": "npm run build:internals:watch"
  },
  "bin": {
    "framework": "bin/framework.bin.js"
  },
  "peerDependencies": {
    "react": "^15.3.1",
    "immutable": "^3.8.1",
    "babel-loader": "^6.2.5",
    "babel-preset-stage-0": "6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react": "^6.11.1",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.9",
    "webpack": "^2.1.0-beta.22",
    "style-loader": "^0.13.1",
    "eventsource-polyfill": "^0.9.6",
    "css-loader": "^0.25.0"
  },
  "dependencies": {
    "babel-cli": "^6.14.0",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "get-port": "^2.1.0",
    "global": "^4.3.0",
    "history": "^4.2.0",
    "intl": "^1.2.4",
    "ip": "^1.1.3",
    "lodash": "^4.15.0",
    "minimist": "^1.2.0",
    "react-dom": "15.3.1",
    "react-intl": "2.1.5",
    "react-redux": "4.4.5",
    "react-router": "2.8.1",
    "react-router-redux": "4.0.5",
    "react-router-scroll": "0.3.2",
    "redux": "3.6.0",
    "redux-immutable": "3.0.8",
    "redux-saga": "0.11.1",
    "regenerator-runtime": "^0.9.5",
    "require-all": "^2.0.0",
    "reselect": "2.5.4",
    "sanitize.css": "4.1.0",
    "semver": "^5.3.0",
    "validator": "^5.5.0",
    "whatwg-fetch": "1.0.0",
    "webpack-dev-middleware": "^1.8.1",
    "webpack-hot-middleware": "^2.12.2",
    "postcss-loader": "0.13.0"
  },
  "devDependencies": {
    "babel-core": "6.14.0",
    "babel-eslint": "6.1.2",
    "babel-plugin-react-intl": "2.2.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-async-to-generator": "^6.8.0",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "6.14.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.5.1",
    "cheerio": "0.22.0",
    "coveralls": "2.11.13",
    "cross-env": "2.0.1",
    "enzyme": "2.4.1",
    "eslint": "^3.5.0",
    "eslint-config-foobarhq": "git@bitbucket.org:foobarhq/eslint-config-foobarhq.git",
    "expect": "1.20.2",
    "expect-jsx": "2.6.0",
    "exports-loader": "0.6.3",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-loader": "0.4.4",
    "html-webpack-plugin": "2.22.0",
    "image-webpack-loader": "2.0.0",
    "imports-loader": "0.6.5",
    "isparta": "4.0.0",
    "isparta-loader": "2.0.0",
    "json-loader": "0.5.4",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-firefox-launcher": "1.0.0",
    "karma-ie-launcher": "1.0.0",
    "karma-mocha": "1.1.1",
    "karma-mocha-reporter": "2.1.0",
    "karma-safari-launcher": "1.0.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "lint-staged": "3.0.2",
    "mocha": "3.0.2",
    "ngrok": "2.2.2",
    "null-loader": "0.1.1",
    "offline-plugin": "3.4.2",
    "plop": "1.5.0",
    "postcss-cssnext": "2.8.0",
    "postcss-focus": "1.0.0",
    "postcss-reporter": "1.4.1",
    "pre-commit": "1.1.3",
    "psi": "2.0.4",
    "rimraf": "2.5.4",
    "shelljs": "0.7.4",
    "sinon": "2.0.0-pre",
    "stylelint": "7.2.0",
    "stylelint-config-standard": "13.0.0",
    "url-loader": "0.5.7"
  }
}
