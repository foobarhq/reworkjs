{
  "name": "react-framework",
  "version": "1.0.0",
  "description": "A framework based on react-boilerplate 3.1.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/mxstbr/react-boilerplate.git"
  },
  "engines": {
    "npm": ">=3"
  },
  "author": "Guylian Cox",
  "license": "MIT",
  "scripts": {
    "preanalyze": "npm run analyze:clean",
    "analyze": "node ./internals/scripts/analyze.js",
    "analyze:clean": "rimraf stats.json",
    "lint": "npm run lint:eslint && npm run lint:css",
    "lint:eslint": "eslint --ignore-path .gitignore --ignore-pattern internals/scripts --ignore-pattern public/",
    "lint:staged": "lint-staged",
    "pretest": "npm run test:clean && npm run lint",
    "test:clean": "rimraf ./coverage",
    "test": "cross-env NODE_ENV=test karma start internals/testing/karma.conf.js --single-run",
    "test:watch": "npm run test -- --auto-watch --no-single-run",
    "test:firefox": "npm run test -- --browsers Firefox",
    "test:safari": "npm run test -- --browsers Safari",
    "test:ie": "npm run test -- --browsers IE",
    "build": "NODE_ENV=node babel src --out-dir=lib/ --copy-files",
    "build:watch": "npm run build -- --watch",
    "coveralls": "cat ./coverage/lcov/lcov.info | coveralls"
  },
  "bin": {
    "framework": "bin/framework.bin.js"
  },
  "peerDependencies": {
    "babel-loader": "^6.2.7",
    "babel-preset-stage-0": "6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react": "^6.16.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-plugin-transform-react-constant-elements": "^6.9.1",
    "babel-plugin-transform-react-inline-elements": "^6.8.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "style-loader": "^0.13.1",
    "css-loader": "^0.25.0"
  },
  "dependencies": {
    "babel-cli": "^6.18.0",
    "bundle-loader": "^0.5.4",
    "chalk": "^1.1.3",
    "compression": "^1.6.2",
    "eslint-loader": "^1.6.1",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.0",
    "get-port": "^2.1.0",
    "global": "^4.3.1",
    "immutable": "^3.8.1",
    "intl": "^1.2.5",
    "ip": "^1.1.4",
    "lodash": "^4.16.6",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "postcss-loader": "^1.1.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-intl": "^2.1.5",
    "react-redux": "^4.4.5",
    "react-router": "^3.0.0",
    "react-router-redux": "^4.0.5",
    "react-router-scroll": "^0.4.1",
    "redux": "^3.6.0",
    "redux-immutable": "^3.0.8",
    "redux-saga": "^0.12.1",
    "regenerator-runtime": "^0.9.6",
    "require-all": "^2.0.0",
    "reselect": "2.5.4",
    "sanitize.css": "4.1.0",
    "semver": "^5.3.0",
    "validator": "^6.1.0",
    "webpack": "^2.1.0-beta.22",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2",
    "whatwg-fetch": "1.0.0",
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-plugin-react-intl": "2.2.0",
    "babel-plugin-react-transform": "2.0.2",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-preset-es2015": "6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "3.5.0",
    "chai-enzyme": "0.6.0",
    "cheerio": "0.22.0",
    "coveralls": "2.11.14",
    "cross-env": "3.1.3",
    "enzyme": "2.5.1",
    "eslint": "^3.9.1",
    "eslint-config-foobarhq": "git@bitbucket.org:foobarhq/eslint-config-foobarhq.git",
    "expect": "1.20.2",
    "expect-jsx": "2.6.0",
    "exports-loader": "0.6.3",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
